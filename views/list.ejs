<%- include('header') -%>
    <div id="heading" class="box">
        <h1><%= listTitle %></h1>
    </div>
    <div class="box">
            <%for(var i=0;i<newListItems.length;i++){%>
                <div class="item">
                    <input type="checkbox" name="" id="">
                    <p><%=newListItems[i]%></p>
                </div>
            <%}%>
            <%if(typeof warning=='undefined' || warning == null){%>
                <%}else{%>
                    <p style="color: orange; text-align: center;"><%=warning%></p>
                    <%}%>
                    
                <form class="" action="/" method="post">
                  <input type="text" name="newItem" placeholder="Add Note" id="" />
                  <br>
                  <input type="text" name="delItem" placeholder="Remove Note"/>
                  <br><br>
                  <button type="submit" name="list" value=<%=listTitle%>>Submit</button>
                </form>
    </div>
    
    <br>
    <%- include('footer') -%>